{
  "name": "Property Power",
  "description": "Giving you the numbers the listing left out.",
  "version": "0.1.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16_gray.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "64": "icon64.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.realestate.com.au/*"],
      "js": ["contentScript.js"],
      "run_at": "document_idle",
      "all_frames": false
    }
  ]
}
